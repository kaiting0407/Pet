generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("LOCAL_DATABASE_URL")
}

model dog {
  dogid   String @id
  dogname String @unique
  habits  String  
  ecoNeeds String
  environment String
  exercise String
  avgage  String
}
